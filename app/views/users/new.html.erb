

<div class="form">
  <h1>Please Sign Up</h1>
  <%= form_for @user do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password%><br>
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %><br>
  <%= f.label :employee_Type %><br>
  <%= f.select(:empl_type, ["Manager", "Server", "Chef"]) %><br>
  <%= f.submit "Sign Up" %><br>
  <% end %>
</div>
